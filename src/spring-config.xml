<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:context="http://www.springframework.org/schema/context"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
    http://www.springframework.org/schema/context
    http://www.springframework.org/schema/context/spring-context-3.0.xsd">

	<context:annotation-config/>    
	
	<bean id="studentbean" class="net.qualifier.bean.StudentBean"/>
    
    <bean id="student1" class="net.qualifier.model.Student">
    	<property name="name" value="demo user"/>
    	<property name="age" value="15"/>
    </bean>
    
     <bean id="student2" class="net.qualifier.model.Student">
    	<property name="name" value="demo user2"/>
    	<property name="age" value="17"/>
    </bean>
    
</beans>